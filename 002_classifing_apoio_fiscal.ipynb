    "import nltk\n",
    "from unidecode import unidecode\n",
    "import string\n",
    "import re\n",
    "\n",
    "# Baixa as stopwords\n",
    "nltk.download('stopwords')\n",
    "stopwords = nltk.corpus.stopwords.words('portuguese')\n",
    "stopwords.extend(['art', 'i', 'ii', 'iii', 'iv', 'v', 'vi', 'vii'])\n",
    "\n",
    "def normalize_text(text):\n",
    "    \"\"\"\n",
    "    Normaliza texto com etapas b치sicas de NLP.\n",
    "    \"\"\"\n",
    "\n",
    "    text = text.lower()       # Min칰sculas\n",
    "    text = unidecode(text)    # Remove acentua칞칚o\n",
    "    text = text.translate(str.maketrans('', '', string.punctuation)) # Remove Pontua칞칚o\n",
    "    text = re.sub(r'\\d+', '', text)\n",
    "    tokens = text.split()\n",
    "\n",
    "    clean_tokens = [token for token in tokens if token not in stopwords and token.isprintable()]\n",
    "\n",
    "    return ' '.join(clean_tokens)\n"
   ]
  },
