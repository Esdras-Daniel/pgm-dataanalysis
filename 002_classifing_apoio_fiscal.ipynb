    "from sklearn.pipeline import Pipeline\n",
    "\n",
    "clf_pipe = Pipeline(steps=[\n",
    "    ('feature-extraction', TfidfVectorizer(preprocessor=normalize_text)),\n",
    "    ('classificator', tree.DecisionTreeClassifier(min_samples_leaf=20, random_state=42))\n",
    "])\n",
    "\n"
   ]
