    "import pandas as pd\n",
    "\n",
    "path = 'data/processed/df_avisos_completos.csv'\n",
    "df = pd.read_csv(path)\n",
    "\n",
    "df['label'] = pd.Series([1 if setor_destino == 'Apoio Fiscal' else 0 for setor_destino in df['setor_destino']])\n",
    "\n",
    "df.head()"
   ]
  },
